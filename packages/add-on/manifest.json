{
  "name": "dat",
  "version": "0.0.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "<all_urls>",
    "webRequest",
    "tabs",
    "webRequestBlocking"
  ],
  "protocol_handlers": [
    {"protocol": "dat", "name": "Dat", "uriTemplate": "http://localhost:4560/%s"}
  ]
}
